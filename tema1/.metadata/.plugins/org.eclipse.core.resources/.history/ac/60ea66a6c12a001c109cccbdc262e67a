package AE_02_T1_2;

import java.awt.event.ActionListener;

public class Controlador {

	Vista vista;
	Modelo modelo;
	
	public Controlador(Vista vista, Modelo modelo) {
		this.vista=vista;
		this.modelo=modelo;
	}
	
	//
	public void initEventHandlers() {
		
		ActionListener actListBtnBuscar= new ActionListener(); 
		this.vista.getBtnBuscar().addActionListener(null);
		
	}
	
	
}
